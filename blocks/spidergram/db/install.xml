<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/spidergram/db" VERSION="20130619" COMMENT="XMLDB file for Moodle blocks/spidergram"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
  <TABLES>
    <TABLE NAME="levelgraph_data" COMMENT="Data regarding Levels for Spidergram">
      <FIELDS>
<FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
<FIELD NAME="level" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Level"/>
<FIELD NAME="fieldid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Field ID"/>
<FIELD NAME="courses" TYPE="char" LENGTH="50" NOTNULL="false" SEQUENCE="false" COMMENT="Courses separated by commas"/>
<FIELD NAME="coursecompletion" TYPE="char" LENGTH="80" NOTNULL="false" SEQUENCE="false" COMMENT="Course Completion - currently not used"/>
<FIELD NAME="timemodified" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" COMMENT="Timestamp"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>